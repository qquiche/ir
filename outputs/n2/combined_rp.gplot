set xlabel "Recall"
set ylabel "Precision"

set terminal postscript color
set size 0.75,0.75

set style data linespoints
set key top right

set xrange [0:1]
set yrange [0:1]

set output "combined_rp.ps"
plot 'control' title "No Feedback" with linespoints lt 1 lc rgb "blue" pt 7, \
     'binary'  title "Binary Feedback" with linespoints lt 2 lc rgb "red"  pt 5, \
     'rated'   title "Rated Feedback"  with linespoints lt 3 lc rgb "green" pt 9
set output
